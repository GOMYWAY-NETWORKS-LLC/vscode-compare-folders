{
  "name": "compare-folders",
  "displayName": "compare-folders",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:foldersCompareAppService"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "view/title": [{
        "command": "foldersCompare.refresh",
        "group": "navigation"
      }]
    },
    "commands": [
      {
        "title": "Choose Folders And Compare",
        "command": "foldersCompare.chooseFoldersAndCompare",
        "enablement": "false"
      },
      {
        "title": "Refresh",
        "command": "foldersCompare.refresh",
        "icon": "resources/refresh.svg"
      }
    ],
    "viewsContainers": {
      "activitybar": [{
        "id": "foldersCompare",
        "title": "Folders Compare",
        "icon": "resources/icon.svg"
      }]
    },
    "views": {
      "foldersCompare": [{
        "id": "foldersCompareAppService",
        "name": "Compare"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.140",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "dir-compare": "^1.7.3",
    "lodash": "^4.17.15",
    "text-diff": "^1.0.1"
  }
}
